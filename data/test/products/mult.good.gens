-- A generator file with generators from the multiplicative monoid (Z/3Z)X(Z/7Z).

MultModP(3,7)

-- Since gcd(2,3)=1 and gcd(3,7)=1, then x and y generate all non-zero elements.
x := (2, 1)
y := (1, 3)

-- An annihilator for each component.
ax := (0, 1)
ay := (1, 0)

-- An arbitrary element in (Z/3Z)X(Z/7Z).
element := (1, 5)

-- The zero-element. Annihilates everything.
zero := (0, 0)
